  "registrationAllowed": false,
  "loginWithEmailAllowed": true,
  "resetPasswordAllowed": true,
  "sslRequired": "none",
  "clients": [
    {
      "clientId": "shiny-public",
      "protocol": "openid-connect",
      "publicClient": true,
      "redirectUris": [
        "http://localhost:3000/*",
        "http://127.0.0.1:3000/*",
        "http://localhost:8100/*",
        "http://127.0.0.1:8100/*"
